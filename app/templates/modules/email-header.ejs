                        <div class="email__subject"><%= subject %></div>
                        <div class="email__meta">
                            <div class="email__meta-item">
                                <strong>Evenitron</strong> &lt;<%= from %>&gt;
                            </div>
                            <div class="email__meta-item">
                                <%= date %>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="email__body">