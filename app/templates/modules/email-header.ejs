        <span class="visuallyhidden">Schedule your interview</span>
    </header>
    <div class="email__nav">
        <a href="/">Back to Demo App</a>
    </div>
    <div class="wrapper__outer wrapper__outer--email">
        <div class="wrapper__inner">
            <form action="/confirm" class="email__container">
                <div class="email__header">
                    <%- include('../icons/envelope', {className: 'email__meta-icon'}); %>
